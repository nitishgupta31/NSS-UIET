doctype html
head
  title Add Events
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1')
  script(src='https://kit.fontawesome.com/e1391d7e22.js')
  link(rel='icon' type='image/png' href='../static/images/icons/favicon.ico')
  link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1' crossorigin='anonymous')
  link(rel='stylesheet' type='text/css' href='../static/main.css')
  link(rel="stylesheet" href="../static/css/style.css") 
  link(rel="stylesheet" href="../static/css/responsive.css")
body
  nav.navbar.navbar-expand-lg.navbar-dark.bg-dark.sticky-top
    .container-fluid
      a.navbar-brand(href='#')
        img(src='../static/img/1.png' height='28' alt='NSS UIET logo')
        |  NSS UIET
      //- a.navbar-brand(href='#') NSS UIET
      button.navbar-toggler(type='button' data-bs-toggle='collapse' data-bs-target='#navbarSupportedContent' aria-controls='navbarSupportedContent' aria-expanded='false' aria-label='Toggle navigation')
        span.navbar-toggler-icon
      #navbarSupportedContent.collapse.navbar-collapse
        ul.navbar-nav.me-auto.mb-2.mb-lg-0
          li.nav-item
              a.nav-link(aria-current='page' href='/') 
                b Home
          li.nav-item
            a.nav-link(href='/eventslist') 
              b Events
          li.nav-item
            a.nav-link(href='/teamlist') 
              b Team
          li.nav-item
            a.nav-link(href='/admin1') 
              b Queries
          li.nav-item
            a.nav-link(href="/adminlist") 
              b Admin List
          li.nav-item.dropdown
            a#navbarDropdown-dark.nav-link.dropdown-toggle(href='' role='button' data-bs-toggle='dropdown' aria-expanded='false')
              | Logout
            ul.dropdown-menu(aria-labelledby='navbarDropdown')
              li
                a.dropdown-item(href='/logout') Logout
              li
                a.dropdown-item(href='/logoutall') Logout from all devices
  .limiter
    .container-login100
      .wrap-login100
        .login100-pic.js-tilt(data-tilt='')
          img(src='../static/images/img-01.png' alt='IMG')
        form(action= "/eventupload", method= "post", class= "form100 login100-form validate-form" enctype='multipart/form-data' )
          if err!=undefined 
            .alert.alert-warning.alert-dismissible.fade.show.mb-0(role='alert')
                strong=err
                button.btn-close(type='button' data-bs-dismiss='alert' aria-label='Close')
          else
          if sucessed!=undefined 
            .alert.alert-success.alert-dismissible.fade.show.mb-0(role='alert')
              strong=sucessed
              button.btn-close(type='button' data-bs-dismiss='alert' aria-label='Close')          
          span.login100-form-title
            | Add New Event

          .wrap-input100.validate-input()
            label.col-md-3.control-label(for=' Description' ) Title
            input.myInput.input100(type='text' name='heading' placeholder='' style="border-radius: 7px 7px; padding: 0 10px 0 10px;" )

          .wrap-input100.validate-input()
            label.col-md-3.control-label(for=' Description') Description
            textarea.myInput.input100(name="description" cols="30" rows="10" placeholder=" " style="height: 77px;resize: none; border-radius: 7px 7px; padding: 0 10px 0 10px;")
          .form-group
            label.control-label(for='file') Upload File
            .col-md-9
            input.form-control(type='file' name='file' required='')
            
          .container-login100-form-btn
            button.login100-form-btn
              | ADD
            
              
  footer.footer100
    p.text-footer100
      | Copyright Â© - NSS UIET - All rights reserved
      br
      | Created by - Nitish Gupta
